# Files to copy
copyfiles=(
    ["."]=".commitlintrc.yml .prettierignore .prettierrc.yml .versionrc .yamllint.yml doc8.ini"
    [".git/hooks"]="commit-msg pre-commit"
)

# OS-independent files to link
# linkfiles=()

# Files to link, depending on OS
